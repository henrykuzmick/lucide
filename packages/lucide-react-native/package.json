{
  "name": "lucide-react-native",
  "description": "A Lucide icon library package for React Native applications",
  "version": "0.16.25",
  "homepage": "https://lucide.dev",
  "bugs": "https://github.com/lucide-icons/lucide/issues",
  "repository": {
    "type": "git",
