    "@svgr/cli": "^6.2.0",
    "@types/react": "^17.0.38",
    "react": "^17.0.2",
    "react-native": "^0.66.4",
    "react-native-svg": "^12.1.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@microsoft/api-extractor": "^7.19.4"
  }
